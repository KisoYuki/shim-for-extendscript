{
  "name": "@kisoyuki/shim-for-extendscript",
  "version": "1.0.0",
  "engines": {
      "node": ">=11.14.0"
  },
  "description": "This module compiles es6 code into extendscript",
  "main": "index.js",
  "bin": {
    "shim-for-extendscript": "./bin/cli.js"
  },
  "scripts": {
    "test": "eslint . --ext .js --ignore-path .gitignore && ava --timeout=10s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KisoYuki/shim-for-extendscript.git"
  },
  "keywords": [
    "shim",
    "extendscript",
    "adobe",
    "es2015",
    "es6",
    "es3",
    "indesign",
    "illustrator",
    "photoshop",
    "aftereffects",
    "compile"
  ],
  "author": "kisoyuki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KisoYuki/shim-for-extendscript/issues"
  },
  "homepage": "https://github.com/KisoYuki/shim-for-extendscript#readme",
  "dependencies": {
    "ava": "^3.7.1",
    "@babel/core": "^7.9.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es5-property-mutators": "^6.24.1",
    "babel-preset-extendscript": "^1.0.2",
    "chokidar": "^3.3.1",
    "eslint": "^7.0.0",
    "eslint-plugin-extendscript": "^1.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-skip-adobe-directives": "^1.0.2",
    "yargs": "^15.3.1"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/src/*",
      "!test/dist/*"
    ],
    "failFast": true
  }
